{
    "require": {
        "cboden/ratchet": "^0.4.3",
        "odan/session": "^5.1",
        "php-di/php-di": "^6.0",
        "react/http": "^1.5",
        "selective/basepath": "^2.1",
        "slim/psr7": "^1",
        "slim/slim": "^4.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "ServerClass\\": "ServerClass"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Test\\": "tests/"
        }
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    }
}
